#include <iostream>
using namespace std;
class Test
{
   private :
            int a, b;
   public :
            Test(int _a, int _B)/> : a(_a), b(_B)/> { }
            int sum() { return(a+B)/>; }
            int display() { cout << sum() << endl; }
};
 
int main()
{
   Test Obj = Test(10, 20);
   Obj.display();
   return (EXIT_SUCCESS);
}
